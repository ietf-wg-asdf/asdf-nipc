PropertyValueReadResponseArray = [* PropertyValueReadResponseArrayItem]

PropertyValueReadResponseArrayItem = ( PropertyValue // FailureResponse )

; PropertyValue = { property, value }
PropertyValue = {
  property: text,   ; SDF global name of the property (absolute URI with fragment)
  value: b64text    ; base64-with-padding encoded bytes (RFC 4648 ยง5)
}

; FailureResponse (Problem Details subset)
FailureResponse = {
  ? type: text,     ; URI of problem type
  ? status: uint,
  ? title: text,
  ? detail: text
}

; Helper type for base64-with-padding encoded text
b64text = text